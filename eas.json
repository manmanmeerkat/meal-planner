{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      },
      "env": {
        "SUPABASE_URL": "@env:SUPABASE_URL",
        "SUPABASE_ANON_KEY": "@env:SUPABASE_ANON_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "SUPABASE_URL": "@env:SUPABASE_URL",
        "SUPABASE_ANON_KEY": "@env:SUPABASE_ANON_KEY"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "SUPABASE_URL": "@env:SUPABASE_URL",
        "SUPABASE_ANON_KEY": "@env:SUPABASE_ANON_KEY"
      }
    }
  }
}
